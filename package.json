    "file-loader": "^6.2.0",
    "sass": "^1.49.7",
    "sass-loader": "10.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.2.0",
    "typescript": "^4.5.5",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@ngneat/effects": "^1.0.0",
    "@ngneat/effects-hooks": "^1.0.0",
    "@ngneat/effects-ng": "^1.0.0",
    "@ngneat/elf": "^1.0.2",
    "@ngneat/elf-cli-ng": "^1.0.0",
