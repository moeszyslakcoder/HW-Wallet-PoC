    "@ngneat/use-observable": "^1.0.0",
    "bip39": "^3.0.4",
    "ethereumjs-wallet": "^1.0.2",
    "ethers": "^5.5.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^6.2.1",
    "sass-resources-loader": "^2.2.4",
    "webcrypto": "^0.1.1"
